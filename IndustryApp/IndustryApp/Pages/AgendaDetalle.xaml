<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="IndustryApp.Pages.AgendaDetalle" Title="Conferencia" BackgroundColor="#E2E2E2">
  <StackLayout x:Name="stack" Orientation="Vertical"  Margin="15">
    <Label x:Name="lblTitulo" FontSize="30" FontAttributes="Bold" TextColor="#023F5E"/>
    <Label x:Name="lblFecha" TextColor="#0080FF" FontSize="13" HorizontalOptions="Center" VerticalOptions="Center" VerticalTextAlignment="Center"/>
    <Label x:Name="lblDescripcion" FontSize="15" TextColor="#313A49" LineBreakMode="WordWrap"/>
  </StackLayout>
</ContentPage>